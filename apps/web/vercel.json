{
  "version": 2,
  "buildCommand": "cd ../.. && pnpm install --frozen-lockfile && pnpm build --filter=@minc-hub/web",
  "outputDirectory": "dist",
  "installCommand": "cd ../.. && pnpm install --frozen-lockfile",
  "framework": "vite",
  "ignoreCommand": "git diff HEAD^ HEAD --quiet apps/web/ ../package.json ../pnpm-lock.yaml ../turbo.json",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css|woff2?|png|jpg|jpeg|svg|gif|ico))$)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
